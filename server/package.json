{
  "name": "gamine",
  "version": "0.1",
  "description": "gamine highscore server",
  "main": "index.js",
  "repository": "https://github.com/abarichello/gamine",
  "author": "Barichello",
  "license": "SEE LICENSE IN LICENSE",
  "private": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build-ts": "yarn run tsc",
    "watch-ts": "tsc -w",
    "serve": "nodemon dist/index.js",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"magenta.bold,grey.italic\" \"yarn run watch-ts\" \"yarn run serve-debug\"",
    "start": "yarn run build-ts && yarn run watch",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "engines": {
    "node": ">=10.0",
    "yarn": ">=1.7.0"
  },
  "dependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/express-promise-router": "^2.0.0",
    "@types/knex": "^0.14.14",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.3.1",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "db-migrate": "^0.11.1",
    "db-migrate-pg": "^0.4.0",
    "dotenv-safe": "^5.0.1",
    "env-node": "^1.0.0",
    "express": "^4.16.3",
    "express-promise-router": "^3.0.2",
    "fs": "^0.0.1-security",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "knex": "^0.14.6",
    "morgan": "^1.9.0",
    "node-db-migrate": "^1.0.1",
    "nodemon": "^1.17.5",
    "source-map-support": "^0.5.6",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.9.1"
  }
}
